{
  "name": "AstraV1",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "bun index.js",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "Rui",
  "license": "LGPL-3.0",
  "dependencies": {
    "axios": "^1.6.8",
    "bluebird": "^2.11.0",
    "chalk": "^4.1.2",
    "cheerio": "^0.22.0",
    "express": "^4.19.2",
    "fca-liane-utils": "^1.2.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "https-proxy-agent": "^4.0.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.45",
    "mqtt": "^3.0.0",
    "npmlog": "^1.2.0",
    "request": "^2.53.0",
    "websocket-stream": "^5.5.0"
  },
  "engines": {
    "node": ">=10.x"
  },
  "devDependencies": {
    "bun": "^1.1.8",
    "eslint": "^7.5.0",
    "mocha": "^7.0.1",
    "prettier": "^1.11.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "es2017": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        1,
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ]
    }
  }
}
